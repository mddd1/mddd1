#include <iostream>

using namespace std;

int main()
{
    string nc[2][4][3];
    for (int i=0;i<2;i++){
    for(int j=0;j<4;j++){
        for(int k=0;k<3;k++){
        cout<<"ingrese su numeros"<<"["<<i<<"]"<<"["<<j<<"]"<<"["<<k<<"]"<<endl;
        cin>>nc[i][j][k];

        }
    }
    }
for (int i=0;i<2;i++){

for(int j=0;j<4;j++){
        for(int k=0;k<3;k++){

        cout<<nc[i][j][k]<<"      ";
        }
        cout<<endl;
    }
cout<<endl;

}


    return 0;
}
